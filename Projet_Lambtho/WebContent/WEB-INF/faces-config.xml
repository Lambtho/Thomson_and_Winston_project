<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/Admin/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ConnectSuccess</from-outcome>
			<to-view-id>/Admin/accueilAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ConnectFail</from-outcome>
			<to-view-id>/Admin/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Admin/*</from-view-id>
		<navigation-case>
			<from-outcome>Accueil</from-outcome>
			<to-view-id>/Admin/accueilAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Succes</from-outcome>
			<to-view-id>/Admin/accueilAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Echec</from-outcome>
			<to-view-id>/Admin/accueilAdmin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>Accueil</from-outcome>
			<to-view-id>/accueil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Succes</from-outcome>
			<to-view-id>/accueil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Echec</from-outcome>
			<to-view-id>/accueil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Recherche</from-outcome>
			<to-view-id>/recherche.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
